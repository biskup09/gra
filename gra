 class Zapis
 {
     public:
     void zapisz()
     {
         FILE * save = fopen ( "save.sav", "w" );
         fprintf( save, "%01d, %01d, 01d, %01d", & lecz, & m_m_lecz, & s_m_lecz, & d_m_lecz, & exp, &
               lvl, & gracz);
               system ("pause");

               fclose ( save );
     }
     void wczytaj()
      {

       FILE * save = fopen( "save.sav", "r");
       fprintf (save, "01d, %01d, %01d, %01d", & lecz, & m_m_lecz, & s_m_lecz, & d_m_lecz, & exp, &
               lvl, & gracz);
               system ("pause");
                fclose (save);

      }

 };

